<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- remotes -->

  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="clang" fetch="git://github.com/kraj" />
  <remote name="zdev" fetch="git://github.com/zuckerman-dev" />
  <remote name="jumpnow" fetch="git://github.com/jumpnow" />
  

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="poky" revision="refs/heads/master" path="geophone" remote="yocto" />
  <project name="meta-raspberrypi" revision="refs/heads/master"  remote="yocto" path="geophone/meta-raspberrypi" />
  <project name="meta-openembedded" revision="refs/heads/master"  remote="oe" path="geophone/meta-openembedded" />
  <project name="meta-clang" revision="refs/heads/master"  remote="clang" path="geophone/meta-clang" />
  <project name="meta-geophone" revision="refs/heads/main"  remote="zdev" path="geophone/meta-geophone" />

</manifest>
