<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- remotes -->

  <remote name="yocto" fetch="https://git.yoctoproject.org" />
  <remote name="oe" fetch="https://git.openembedded.org" />
  <remote name="clang" fetch="https://github.com/kraj" />
  <remote name="zdev" fetch="https://github.com/zuckerman-dev" />
  <remote name="mkts" fetch="https://github.com/miketsukerman" />
  <remote name="sbabic" fetch="https://github.com/sbabic" />
  <remote name="ros" fetch="https://github.com/ros/" />
  

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="poky" revision="refs/heads/kirkstone" path="geophone" remote="yocto" />
  <project name="meta-raspberrypi" revision="refs/heads/kirkstone"  remote="yocto" path="geophone/meta-raspberrypi" />
  <project name="meta-openembedded" revision="refs/heads/kirkstone"  remote="oe" path="geophone/meta-openembedded" />
  <project name="meta-clang" revision="refs/heads/kirkstone"  remote="clang" path="geophone/meta-clang" />
  <project name="meta-geophone" revision="refs/heads/kirkstone"  remote="zdev" path="geophone/meta-geophone" />
  <project name="meta-swupdate" revision="refs/heads/kirkstone" remote="sbabic" path="geophone/meta-swupdate" />
  <project name="meta-ros" revision="refs/heads/kirkstone" remote="mkts" path="geophone/meta-ros" />

</manifest>
